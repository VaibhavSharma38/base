#user-profile.xepan-contact-profile
  .main-box.clearfix
    header.main-box-header.clearfix
      h2 {first_name}First Name{/} 
      h2 {last_name}Last Name{/} 
    .main-box-body.clearfix
      .profile-status
        i.fa.fa-circle
        |  Online
      {avatar}
      img.profile-img.img-responsive.center-block(alt='', src='')
      {/}
      .profile-label
        span.label.label-danger {type}{/}
      .profile-since
        | Contact since: {created_at}{/}
      <hr/>
      h2 Emails
      .profile-details
        {Emails}
        {rows}{row}
        {$header}
        {/}{/}
        {not_found_message}{/}
        {/}
      h2 Phones
      .profile-details
        {Phones}
        {/}
      .profile-message-btn.center-block.text-center
        .btn-group.btn-group-sm(role="group")
          button.btn.btn-primary(type="button" title="send email")
            i.fa.fa-envelope
            |&nbsp;Email
          button.btn.btn-primary(type="button" title="send message")
            i.fa.fa-phone
            |&nbsp;SMS
      .row.xepan-push
      .row      
      .profile-details
        .form-group
          span(title="organization")
            i.fa.fa-building.fa-lg &nbsp;
            label Organization
            |{organization}-{/}
        .form-group
          span(title="post")
            i.fa.fa-user.fa-lg &nbsp;
            label Post
            |{post}-{/}
        .form-group
          span(title="website")
            i.fa.fa-globe.fa-lg &nbsp;
            label Website
            |{website}-{/}
        .form-group
          span(title="address")
            i.fa.fa-map-marker.fa-lg &nbsp;
            label Address
            |{address}not found{/}<br/>
            label Country: 
            div {country_id}not found{/}<br/>
            label State: 
            div {state_id}not found{/}<br/>
            label City: 
            {city}not found{/}<br/>
            label Pin Code: 
            {pin_code}not found{/}
      h2 IM Numbers
      .profile-details 
        {IMs}
        div
          span(title="Skype")
            i.fa.fa-skype &nbsp;
              a(href="wwww.xavoc.com" target="_blank")  
        div
          span(title="Whatsup")
            i.fa.fa-whatsapp.fa-phone &nbsp;
              a(href="9090909090" target="_blank") 
        
        {/}
      h2 Events
      .profile-details 
        {Events}
        div
          span(title="DOB")
            i.fa.fa-wish &nbsp;
          |DOB:
        {/}
      h2 Relationship
      .profile-details 
        {Relations}
        div
          span(title="Father")
            i.fa.fa-skype &nbsp;
          | 
        {/}